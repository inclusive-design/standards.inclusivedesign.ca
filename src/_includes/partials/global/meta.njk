<meta charset="utf-8">
<meta name="viewport" content="width=device-width,initial-scale=1">
<meta name="author" content="{{ site.author }}">
<meta name="description" content="{{ metaExcerpt if metaExcerpt else site.description }}">
<meta name="og:type" property="og:type" content="website">
<meta name="og:site_name" property="og:site_name" content="{{ site.title }}">
<meta name="og:description" property="og:description" content="{{ metaExcerpt if metaExcerpt else site.description }}">
<meta property="og:title" content="{{ title }}">
<meta property="og:url" content="{{ page.url }}">